def double_max(n):
    answer = [] + [n[0]]
    for probnii in n:
        if answer[0] != probnii:
            answer.append(probnii)
            break

    if len(answer) == 2:
        for number in n:
            if number in answer:
                continue
            delta_1 = number - answer[0]
            delta_2 = number - answer[1]

            if delta_1 > delta_2 and delta_1 > 0:
                answer[0] = number
            elif delta_2 > delta_1 and delta_2 > 0:
                answer[1] = number

        return answer

    else:
        return 'ERROR'
